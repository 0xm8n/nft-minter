{
  "name": "nftmint",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "lint": "prettier test/**/* examples/**/* scripts/**/* --check",
    "lint:fix": "prettier test/**/* examples/**/* scripts/**/* --write",
    "format": "prettier --check contracts/**/*.sol examples/**/*.sol",
    "format:fix": "prettier --write contracts/**/*.sol examples/**/*.sol",
    "clean": "hardhat clean",
    "build": "hardhat compile",
    "rebuild": "hardhat clean && hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat --network ropsten run scripts/deploy.ts",
    "verify": "hardhat verify --network ropsten --constructor-args scripts/arguments.js",
    "main:deploy": "hardhat --network mainnet deploy",
    "main:verify": "hardhat --network mainnet etherscan-verify --solc-input --license GPL-3.0 --force-license",
    "test:gas": "cross-env REPORT_GAS=true hardhat test",
    "mint": "hardhat --network ropsten run scripts/mint.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@alch/alchemy-web3": "^1.1.11",
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/contracts": "^4.4.2",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.3",
    "hardhat-gas-reporter": "^1.0.7",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.18",
    "ts-node": "^10.4.0",
    "typechain": "^5.2.0",
    "typescript": "^4.5.5"
  }
}
